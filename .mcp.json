{
  "mcpServers": {
    "codex_subagent": {
      "type": "stdio",
      "command": "uvx",
      "args": ["codex-as-mcp@latest"],
      "env": {
        "MCP_SERVER_REQUEST_TIMEOUT": "300000",
        "MCP_REQUEST_TIMEOUT_RESET_ON_PROGRESS": "True"
      },
      "includeTools": [],
      "excludeTools": []
    },
    "postgres": {
      "type": "stdio",
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-postgres",
        "postgresql://postgres:postgres@127.0.0.1:54322/postgres"
      ],
      "env": {},
      "includeTools": [],
      "excludeTools": []
    },
    "sequential-thinking": {
      "type": "stdio",
      "command": "npx",
      "args": ["-y", "@modelcontextprotocol/server-sequential-thinking"],
      "env": {},
      "includeTools": [],
      "excludeTools": []
    },
    "serena": {
      "type": "stdio",
      "command": "uvx",
      "args": [
        "--from",
        "git+https://github.com/oraios/serena",
        "serena",
        "start-mcp-server",
        "--context",
        "codex"
      ],
      "env": {},
      "includeTools": [],
      "excludeTools": [
        "insert_after_symbol",
        "insert_before_symbol",
        "delete_memory",
        "replace_symbol_body",
        "onboarding"
      ]
    }
  }
}
