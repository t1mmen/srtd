{
  "extends": "@sindresorhus/tsconfig",
  "compilerOptions": {
    "moduleResolution": "bundler",
    "module": "ES2022",
    "outDir": "dist",
    "sourceMap": true,
    "noUncheckedIndexedAccess": false,
    "noPropertyAccessFromIndexSignature": false
  },
  "include": ["src"],
  "exclude": ["node_modules", "dist"]
}
